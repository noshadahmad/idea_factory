
<h1>Sign In Form </h1>
<%= form_with url: session_path, local: true do |f| %>
    <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
    </div>
    <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
    </div>
    <div>
    <%= f.submit "Sign In" %>
    </div>

    <% end %>